[alias]
  st = status
  tip = log --oneline --decorate --reverse origin/master..HEAD
  ci = commit
  br = branch
  co = checkout
  df = diff

[core]
    attribsfile = ~/.gitattributes

[color]
    ui = auto

[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green

[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold

[color "status"]
    added = yellow
    changed = green
    untracked = cyan

[merge]
    log = true

[branch "master"]
    remote = origin
    merge = refs/head/master

[user]
    name = Kevin Kragenbrink
